{
  "name": "deployee/deployee",
  "description": "Taskrunner for PHP",
  "minimum-stability": "stable",
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Phillip Schleicher",
      "email": "the@phizzl.de"
    }
  ],
  "bin": ["bin/deployee"],
  "require": {
    "php": ">=7.0",
    "ext-pdo": "*",
    "deployee/container": "^1.0",
    "deployee/plugins": "^1.0",
    "deployee/application": "^1.0",
    "deployee/config": "^1.0",
    "deployee/dependency": "^1.0",
    "deployee/environment": "^1.0",
    "deployee/plugin-deploy": "^1.0",
    "deployee/plugin-install": "^1.0",
    "deployee/plugin-shelltasks": "^1.0",
    "deployee/plugin-filesystemtasks": "^1.0",
    "deployee/docblock": "^1.0",
    "deployee/plugin-deployenv": "^1.0",
    "deployee/persistence": "^1.0",
    "deployee/plugin-deployhistory": "^1.0",
    "deployee/plugin-dbtasks": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Deployee\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Deployee\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5",
    "roave/security-advisories": "dev-master",
    "deployee/plugin-idesupport": "^1.0",
    "deployee/plugin-generatedeploy": "^1.0"
  }
}
