{
  "name": "deployee/deployee",
  "description": "Taskrunner for PHP",
  "minimum-stability": "stable",
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Phillip Schleicher",
      "email": "the@phizzl.de"
    }
  ],
  "bin": ["bin/deployee"],
  "require": {
    "php": ">=7.3",
    "ext-pdo": "*",
    "deployee/container": "^2.0",
    "deployee/plugins": "^2.0",
    "deployee/application": "^2.0",
    "deployee/config": "^2.0",
    "deployee/dependency": "^2.0",
    "deployee/environment": "^2.0",
    "deployee/plugin-deploy": "^2.0",
    "deployee/plugin-install": "^2.0",
    "deployee/plugin-shelltasks": "^2.0",
    "deployee/plugin-filesystemtasks": "^2.0",
    "deployee/docblock": "^2.0",
    "deployee/plugin-deployenv": "^2.0",
    "deployee/persistence": "^2.0",
    "deployee/plugin-deployhistory": "^2.0",
    "deployee/plugin-dbtasks": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Deployee\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Deployee\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0.0",
    "roave/security-advisories": "dev-master",
    "deployee/plugin-idesupport": "^2.0",
    "deployee/plugin-generatedeploy": "^2.0"
  }
}
