{
  "name": "deployee/deployee",
  "description": "Taskrunner for PHP",
  "minimum-stability": "stable",
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Phillip Schleicher",
      "email": "the@phizzl.de"
    }
  ],
  "bin": ["bin/deployee"],
  "require": {
    "php": ">=8.0",
    "ext-pdo": "*",
    "deployee/container": "3.0.0",
    "deployee/plugins": "3.0.0",
    "deployee/application": "3.0.0",
    "deployee/config": "3.0.0",
    "deployee/dependency": "3.0.0",
    "deployee/environment": "3.0.0",
    "deployee/plugin-deploy": "3.0.0",
    "deployee/plugin-install": "3.0.0",
    "deployee/plugin-shelltasks": "3.0.0",
    "deployee/plugin-filesystemtasks": "3.0.0",
    "deployee/docblock": "3.0.0",
    "deployee/plugin-deployenv": "3.0.0",
    "deployee/persistence": "3.0.0",
    "deployee/plugin-deployhistory": "3.0.0",
    "deployee/plugin-dbtasks": "3.0.0"
  },
  "autoload": {
    "psr-4": {
      "Deployee\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Deployee\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0.0",
    "roave/security-advisories": "dev-master",
    "deployee/plugin-idesupport": "^2.0",
    "deployee/plugin-generatedeploy": "^2.0"
  }
}
