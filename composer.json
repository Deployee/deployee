{
  "name": "deployee/deployee",
  "description": "Taskrunner for PHP",
  "minimum-stability": "stable",
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Phillip Schleicher",
      "email": "the@phizzl.de"
    }
  ],
  "bin": ["bin/deployee"],
  "require": {
    "php": ">=7.0",
    "ext-pdo": "*",
    "deployee/plugins": "v1.1.x-dev",
    "deployee/environment": "^1.0",
    "symfony/dependency-injection": "^3.4",
    "symfony/proxy-manager-bridge": "^3.4",
    "symfony/config": "^3.4",
    "symfony/console": "^3.4",
    "symfony/finder": "^3.4",
    "symfony/event-dispatcher": "^3.4",
    "deployee/plugin-install": "v1.1.x-dev"
  },
  "autoload": {
    "psr-4": {
      "Deployee\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Deployee\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5",
    "roave/security-advisories": "dev-master"
  }
}
