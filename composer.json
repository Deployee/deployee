{
  "name": "phizzl/deployee",
  "description": "Taskrunner for PHP",
  "minimum-stability": "stable",
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Phillip Schleicher",
      "email": "the@phizzl.de"
    }
  ],
  "bin": ["bin/deployee"],
  "require": {
    "php": ">=7.0",
    "symfony/event-dispatcher": "^3.3",
    "symfony/console": "^3.4",
    "pimple/pimple": "^3.2",
    "monolog/monolog": "^1.24",
    "phizzl/php-mysql-command-builder": "^0.2",
    "phpdocumentor/reflection-docblock": "^3.2",
    "symfony/yaml": "^4.2",
    "symfony/finder": "^4.2"
  },
  "autoload": {
    "psr-4": {
      "Deployee\\": "src/",
      "Deployee\\Plugins\\": "plugins/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Deployee\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "roave/security-advisories": "dev-master"
  }
}
